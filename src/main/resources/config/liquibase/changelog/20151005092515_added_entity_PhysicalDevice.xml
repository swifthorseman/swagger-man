<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">

    <property name="now" value="now()" dbms="mysql,h2"/>
    <property name="now" value="current_timestamp" dbms="postgresql"/>
    <property name="now" value="sysdate" dbms="oracle"/>

    <property name="autoIncrement" value="true" dbms="mysql,h2,postgresql"/>
    <property name="autoIncrement" value="false" dbms="oracle"/>

    <property name="floatType" value="float4" dbms="postgresql, h2"/>
    <property name="floatType" value="float" dbms="mysql, oracle"/>

    <!--
        Added the entity PhysicalDevice.
    -->
    <changeSet id="20151005092515" author="jhipster">
        <createTable tableName="PHYSICAL_DEVICE">
            <column name="id" type="bigint" autoIncrement="${autoIncrement}" >
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="device_model_identifier" type="varchar(255)"/>
            <column name="device_model_name" type="varchar(255)"/>
            <column name="serial_number" type="varchar(255)"/>
            <column name="payment_capture_device_identifier" type="varchar(255)"/>
            <column name="gateway_identifier" type="varchar(255)"/>
            <column name="party_level" type="varchar(255)"/>
            <column name="applied_to_party_identifier" type="varchar(255)"/>
            <column name="party_role_identifier" type="varchar(255)"/>
            <column name="party_role_name" type="varchar(255)"/>
            <column name="device_status_identifier" type="varchar(255)"/>
            <column name="device_status_name" type="varchar(255)"/>
            <column name="device_firmware_version_identifier" type="varchar(255)"/>
            <column name="device_firmware_version" type="varchar(255)"/>
            <column name="device_manufacturer_identifier" type="varchar(255)"/>
            <column name="device_manufacturer_name" type="varchar(255)"/>
            <column name="p2_pecompliance_flag" type="varchar(255)"/>
            <column name="device_ownership_flag" type="varchar(255)"/>
        </createTable>
        
    </changeSet>
</databaseChangeLog>
